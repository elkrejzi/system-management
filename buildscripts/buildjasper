#!/bin/bash -e

export PKGNAME=jasper
export PKGVER=1.900.16
export PKGTAR=${PKGNAME}-${PKGVER}.tar.gz
export PKGURL="http://www.ece.uvic.ca/~frodo/jasper/software/${PKGTAR}"
export MAKE_JOBS_FLAGS="-j4"
export INSTALL_CMD=("for dir in usr/include usr/lib ; do cp -rf --remove-destination \$dir /usr ; done")
export CONFIGURE_FLAGS=(--enable-shared)

. $(dirname $0)/master.sh
