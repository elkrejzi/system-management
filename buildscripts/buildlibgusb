#!/bin/bash -e

export PKGNAME=libgusb
export PKGVER=0.2.8
export PKGTAR=${PKGNAME}-${PKGVER}.tar.xz
export PKGURL="http://people.freedesktop.org/~hughsient/releases/${PKGTAR}"
export DEST=/binary/gnome/${PKGNAME}-${PKGVER}
export MAKE_JOBS_FLAGS="-j4"

export DEFAULT_CC="clang"

. $(dirname $0)/master.sh
